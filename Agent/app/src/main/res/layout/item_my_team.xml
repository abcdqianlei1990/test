<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/item_my_team_supperAgentName_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/sp_15"
        android:text="姓名："
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="12dp"
        android:layout_marginLeft="22dp"/>
    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/item_my_team_supperAgentPhone_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/sp_15"
        android:text=""
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="12dp"
        android:layout_marginRight="20dp"/>
    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/item_my_team_supperAgentNickName_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="@dimen/sp_15"
        android:text="昵称："
        android:textColor="@color/black666666"
        app:layout_constraintTop_toBottomOf="@+id/item_my_team_supperAgentName_tv"
        app:layout_constraintLeft_toLeftOf="@+id/item_my_team_supperAgentName_tv"
        app:layout_constraintRight_toLeftOf="@+id/item_my_team_entryDate_tv"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="12dp"
        android:visibility="visible"
        />
    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/item_my_team_entryDate_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/item_my_team_supperAgentNickName_tv"
        app:layout_constraintBottom_toBottomOf="@+id/item_my_team_supperAgentNickName_tv"
        app:layout_constraintRight_toRightOf="@+id/item_my_team_supperAgentPhone_tv"
        android:text="加入时间："
        android:textSize="@dimen/sp_12"
        android:textColor="@color/black666666"/>
</android.support.constraint.ConstraintLayout>